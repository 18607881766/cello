---
{% for orderer in allorderers %}
- targets:
  - {{ allips|random }}:{{ k8sports[orderer.name+':8443'] }}
  labels:
    hostname: "{{ orderer.name }}"
{% endfor %}
{% for peer in allpeers %}
- targets:
  - {{ allips|random }}:{{ k8sports[peer.name+':9443'] }}
  labels:
    hostname: "{{ peer.name }}"
{% endfor %}

